<!DOCTYPE html>
<html>
  <head>
    <title>Dashboard</title>

    
    </style>
  </head>

  <body>

    <!-- TABLE -->
<div>

  
  <div class="container px-4 px-lg-5">
    <div class="row gx-4 gx-lg-5 justify-content-center">
      <div class="col-12 col-lg-11 col-xl-10" >
        
        <% lists.forEach(p => { %>
       <div class="post-preview" style="background-image: url('/<%= p.image %>')" >
            <a href="/client/viewBlog/<%= p._id %>"><h2 class="post-title"><%= p.title %></h2>
            <h3 style="color: aqua;"><%= p.category.name %></h3></a>
            <p class="post-meta">
              Posted by
              <a href="#!">Start Bootstrap</a>
              on September 18, 2023
            </p>
          </div>
          <!-- Divider-->
          <hr class="my-4" />
          <% }) %>
      
    </div>
    </div>
    </div>
    </div>

  </body>
</html>
